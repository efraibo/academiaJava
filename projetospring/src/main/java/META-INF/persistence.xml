<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="projetospring">
		
		<class>org.springframework.data.jpa.domain.AbstractPersistable</class>
		<class>br.com.senai.model.User</class>
		<class>br.com.senai.model.Usuario</class>
        <class>br.com.senai.model.MeioDeComunicacao</class>
        <class>br.com.senai.model.Titulacao</class>
        <class>br.com.senai.model.Telefone</class>
        <class>br.com.senai.model.Endereco</class>
        <class>br.com.senai.model.Pessoa</class>
        <class>br.com.senai.model.Publicacao</class>
        <class>br.com.senai.model.Professor</class>
        <class>br.com.senai.model.Aluno</class>           
        
		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect" />
		</properties>
	
	</persistence-unit>
</persistence>
